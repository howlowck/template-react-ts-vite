{
  "name": "template-react-ts-vite",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run clean && npm run build:be && npm run build:fe",
    "build:be": "tsc -p tsconfig.server.json",
    "build:fe": "tsc && vite build",
    "build:storybook": "build-storybook",
    "clean": "rimraf ./dist",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:fe\"",
    "dev:api": "tsnd --project tsconfig.server.json --respawn server/index.ts",
    "dev:fe": "vite",
    "make": "plop",
    "test": "npm run test:fe",
    "test:fe": "jest --config jest.config.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "body-parser": "^1.19.1",
    "classnames": "^2.3.1",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-interactions": "^6.4.13",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/jest": "^0.0.5",
    "@storybook/react": "^6.4.12",
    "@storybook/testing-library": "^0.0.7",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.22",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "cross-fetch": "^3.1.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.5.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "mkdirp": "^1.0.4",
    "msw": "^0.36.4",
    "plop": "^3.0.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
