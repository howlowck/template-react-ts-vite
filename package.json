{
  "name": "template-react-ts-vite",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run clean && npm run build:be && npm run build:fe",
    "build:be": "tsc -p tsconfig.server.json",
    "build:fe": "tsc && vite build",
    "build:storybook": "build-storybook",
    "clean": "rimraf ./dist",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:fe\"",
    "dev:api": "tsnd --project tsconfig.server.json --respawn server/index.ts",
    "dev:fe": "vite",
    "make": "plop",
    "test": "npm run test:fe",
    "test:fe": "jest --config jest.config.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "body-parser": "^1.20.2",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.3",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "^7.1.30",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mkdirp": "^3.0.1",
    "msw": "^2.0.4",
    "plop": "^4.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  }
}
