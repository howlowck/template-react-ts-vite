{
  "name": "template-react-ts-vite",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "^20.9.0",
    "npm": "^10.1.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "start": "node dist/index.cjs",
    "build": "npm run clean && npm run build:be && npm run build:fe",
    "build:be": "pkgroll --src ./server",
    "build:fe": "vite build",
    "build:storybook": "build-storybook",
    "clean": "rimraf ./dist",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:fe\"",
    "dev:api": "nodemon --watch server -e ts,tsx --exec 'pkgroll --src ./server && node dist/index.cjs'",
    "dev:fe": "vite",
    "make": "plop",
    "test": "npm run test:fe",
    "test:fe": "vitest",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "body-parser": "^1.20.2",
    "classnames": "^2.5.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "redux-observable": "3.0.0-rc.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@sindresorhus/tsconfig": "^5.0.0",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.1.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "happy-dom": "^12.10.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "mkdirp": "^3.0.1",
    "msw": "^2.0.11",
    "nodemon": "^3.0.2",
    "pkgroll": "^2.0.1",
    "plop": "^4.0.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
