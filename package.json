{
  "name": "template-react-ts-vite",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run clean && npm run build:be && npm run build:fe",
    "build:be": "tsc -p tsconfig.server.json",
    "build:fe": "tsc && vite build",
    "build:storybook": "build-storybook",
    "clean": "rimraf ./dist",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:fe\"",
    "dev:api": "tsnd --project tsconfig.server.json --respawn server/index.ts",
    "dev:fe": "vite",
    "make": "plop",
    "test": "npm run test:fe",
    "test:fe": "jest --config jest.config.js",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "body-parser": "^1.20.1",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-loader": "^9.1.0",
    "concurrently": "^7.6.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "mkdirp": "^1.0.4",
    "msw": "^0.49.2",
    "plop": "^3.1.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1"
  }
}
